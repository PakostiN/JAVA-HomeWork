import java.util.Scanner;

public class Zad1 {

	public static void main(String[] args) {
		System.out.println("Ќапишете програма, ко€то показва знака (+ или -) от частното на две реални числа, без да го пресм€та.");
		Scanner test = new Scanner (System.in);
		int delimo = test.nextInt();
		int delitel = test.nextInt();
		int result = 0;
		result = delimo/delitel;
		if (result < 0){
			System.out.println(" - ");
		}
		else (result > 0){
			System.out.println(" + ");
		}
		test.close();


	}

}