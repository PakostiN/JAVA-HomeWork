
public class Zad9 {

	public static void main(String[] args) {
		System.out.println("Напишете програма, която обръща стойностите на един масив.");
		int[] MyArray = { 1, 2, 3, 4, 5 };
		for (int i = 4; i < MyArray.length; i--) {
			if (i < 0) {
				break;
			} else {
				System.out.println(MyArray[i]);
			}

		}
	}
}
