
public class Zad9 {

	public static void main(String[] args) {
		System.out.println("Напишете програма, която обръща стойностите на един масив.");
		int[] MyArray = {1,2,3,4,5};
		for (int i = 0; i < MyArray.length; i++) {
			for (int j = 4; j == MyArray.length; j--) {
				System.out.print(j + " ");
				
			}
		}
	}

}
